<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kosmo.shoong.admin.mybatis.packAdminsql">
	
	
	<select id="packList" resultType="java.util.Map" >
		SELECT * FROM PACK
	</select>
	
	<delete id="packDelete" parameterType="java.util.Map">
		DELETE FROM PACK WHERE PACK_ID = #{packID}
	</delete>
</mapper>